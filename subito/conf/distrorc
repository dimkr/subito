# the distribution name
DISTRO_NAME="Subito GNU/Linux"

# the distribution nickname, used as a prefix for file names and the hostname
DISTRO_NICKNAME="subito"

# the distribution version, in a nicely-printable representation
DISTRO_VERSION="1.0.0 Stable"

# the distribution version, in a numeric representation
DISTRO_VERSION_NUMERIC="1.0.0"

# the distribution processor architecture
DISTRO_ARCH="x86_64"
